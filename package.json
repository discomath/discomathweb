{
  "name": "discomathweb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "format": "prettier --write src/**/*",
    "lint:md": "markdownlint --ignore \"**/node_modules/**\" .",
    "start": "react-scripts start"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.7",
    "@material-ui/icons": "^4.9.1",
    "clsx": "^1.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.3",
    "@vapurrmaid/editor-config": "^0.2.2",
    "@vapurrmaid/md-config": "^0.2.0",
    "@vapurrmaid/prettier-config": "^0.1.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.9",
    "markdownlint-cli": "^0.22.0",
    "prettier": "^2.0.2",
    "react-scripts": "3.4.1",
    "typescript": "~3.7.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": "yarn lint:md",
    "*.{css,json,html,md,ts,tsx,yaml,yml}": "prettier --write"
  }
}
